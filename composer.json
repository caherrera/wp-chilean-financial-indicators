{
  "name": "caherrera/wp-chilean-financial-indicators",
  "type": "library",
  "description": "A WP Plugin",
  "license": "MIT",
  "support": {
    "docs": "https://github.com/caherrera/wp-chilean-financial-indicators/blob/master/readme.md",
    "issues": "https://github.com/caherrera/wp-chilean-financial-indicators/issues",
    "source": "https://github.com/caherrera/wp-chilean-financial-indicators",
    "email": "caherrera@gmail.com"
  },
  "authors": [
    {
      "name": "Carlos Herrera",
      "email": "caherrera@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "mockery/mockery": "~1.0"
  },
  "archive": {
    "exclude": [
      "/examples",
      "/tests"
    ]
  },
  "autoload": {
    "psr-4": {
      "ChileanIndicator\\": "inc/",
      "ChileanIndicatorWidget\\": "widgets/"
    }
